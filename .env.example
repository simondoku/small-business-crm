# Example Environment Variables - Copy to .env for local development

# API Configuration
REACT_APP_API_URL=http://localhost:5003/api
PUBLIC_URL=http://localhost:3000

# Stripe Integration - Replace with your own keys
REACT_APP_STRIPE_PUBLISHABLE_KEY=pk_test_your_publishable_key_here

# Feature Flags
REACT_APP_ENABLE_ANALYTICS=false
REACT_APP_ENABLE_NOTIFICATIONS=true

# Performance Settings
REACT_APP_CACHE_DURATION=300000 # 5 minutes in milliseconds

# Debug/Monitoring
REACT_APP_ENABLE_ERROR_REPORTING=false
REACT_APP_LOG_LEVEL=info # Options: debug, info, warn, error
